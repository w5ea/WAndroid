        <activity android:name=".activities.viewpager.ViewPagerUsage"></activity>
        <activity android:name=".imageloader.displayingbitmaps.ui.ImageDetailActivity2"></activity>
        <activity android:name=".animation.AnimationUsage"></activity>
        <activity android:name=".text.TextUseage"></activity>
        <activity android:name=".applation.ApplationInfoUsage"></activity>
        <activity android:name=".activities.residemenu.ResideMenuUsage"></activity>
        <activity android:name=".slidingmenu.usage.SlidingMenuUsage"></activity>
        <activity android:name=".shapeimageview.ShapeImageViewUsage"></activity>
